new file mode 100644
index 00000000..778c586a
--- /dev/null
+++ b/jenkins/benchmarks/radargun/quickstart-radargun1.sh
@@ -0,0 +1,25 @@
+#!/bin/bash
+#############################################################################################################################
+#  <description>according to http://radargun.github.io/radargun/measuring_performance/understanding_results.html&#xd;
+#SOME results are  bigger==better bot OTHER TWO are smaller==better&#xd;
+#namely:&#xd;
+#  LATENCY - bigger = worse&#xd;
+#  OTHERS  - bigger = better&#xd;
+#</description>
+#############################################################################################################################
+set -e
+set -o pipefail
+
+## resolve folder of this script, following all symlinks,
+## http://stackoverflow.com/questions/59895/can-a-bash-script-tell-what-directory-its-stored-in
+SCRIPT_SOURCE="${BASH_SOURCE[0]}"
+while [ -h "$SCRIPT_SOURCE" ]; do # resolve $SOURCE until the file is no longer a symlink
+  SCRIPT_DIR="$( cd -P "$( dirname "$SCRIPT_SOURCE" )" && pwd )"
+  SCRIPT_SOURCE="$(readlink "$SCRIPT_SOURCE")"
+  # if $SOURCE was a relative symlink, we need to resolve it relative to the path where the symlink file was located
+  [[ $SCRIPT_SOURCE != /* ]] && SCRIPT_SOURCE="$SCRIPT_DIR/$SCRIPT_SOURCE"
+done
+readonly SCRIPT_DIR="$( cd -P "$( dirname "$SCRIPT_SOURCE" )" && pwd )"
+source $SCRIPT_DIR/../../../tck/autoruns/common.sh
+
+bash $SCRIPT_DIR/quickstart-radargun.sh 1
